{
  "name": "chinesenamefinder",
  "version": "0.1.0",
  "description": "AI-powered Chinese name generator platform",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "validate-env": "node scripts/validate-env.mjs",
    "build:opennext": "node scripts/validate-env.mjs && npx @opennextjs/cloudflare@latest build --openNextConfigPath open-next.config.js && node scripts/fix-cloudflare-assets.mjs && node scripts/postbuild-cloudflare.mjs && ls -la .open-next/",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "pages:deploy": "wrangler pages deploy dist",
    "pages:dev": "wrangler pages dev dist",
    "db:generate": "supabase gen types typescript --project-id $SUPABASE_PROJECT_ID --schema public > lib/database.types.ts",
    "deploy": "bash scripts/deploy.sh"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.1.5",
    "@sentry/nextjs": "^8.0.0",
    "@supabase/auth-ui-react": "^0.4.6",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/supabase-js": "^2.38.5",
    "autoprefixer": "^10.4.16",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.294.0",
    "next": "^15.0.3",
    "next-intl": "^4.3.9",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sonner": "^1.2.4",
    "stripe": "^14.9.0",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231121.0",
    "@opentelemetry/api": "^1.9.0",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "eslint": "^8.55.0",
    "eslint-config-next": "^15.0.3",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.2",
    "webpack": "^5.101.3",
    "wrangler": "^4.37.0"
  },
  "overrides": {
    "path-to-regexp": "^8.2.0",
    "esbuild": "^0.25.9",
    "undici": "^6.0.0",
    "cookie": "^0.7.0",
    "wrangler": {
      "esbuild": "^0.25.9"
    }
  },
  "engines": {
    "node": ">=18.17.0"
  }
}
